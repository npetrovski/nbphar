<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Loaders">
        <folder name="application">
            <folder name="x-phar">
                <folder name="Factories">
                    <file name="PharArchiveDataLoader.instance">
                        <attr name="dataObjectClass" stringvalue="name.npetrovski.nbphar.PharArchiveDataObject"/>
                        <attr name="iconBase" stringvalue="name/npetrovski/nbphar/phar.png"/>
                        <attr name="instanceCreate" methodvalue="org.openide.loaders.DataLoaderPool.factory"/>
                        <attr name="mimeType" stringvalue="application/x-phar"/>
                    </file>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Services">
        <folder name="MIMEResolver">
            <file name="PharArchiveResolver.xml" url="PharArchiveResolver.xml">
                <attr name="displayName" bundlevalue="name.npetrovski.nbphar.Bundle#Resolver"/>
            </file>
        </folder>
    </folder>
</filesystem>
